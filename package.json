{
  "name": "slack-chat",
  "main": "./lib/slack-chat",
  "version": "1.0.0",
  "description": "Slack Chat is an Atom package that integrates the slack messaging client into the editor.",
  "activationEvents": [
    "slack-chat:toggle",
    "slack-chat:send-message",
    "slack-chat:unfocus",
    "slack-chat:focus",
    "slack-chat:open-conversation",
    "slack-chat:close-conversation",
    "slack-chat:jump-to-previous-conversation",
    "slack-chat:jump-to-next-conversation"
  ],
  "repository": "https://github.com/callahanrts/slack-chat",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "body-parser": "1.3.0",
    "express": "~4.3.0",
    "jquery": "~1.11.3",
    "jquery-scrollto": "*",
    "loophole": "^1.0.0",
    "underscore-plus": "~1.2.1"
  }
}
